list_str=(input()).split()
person=0
list=[int(item) for item in list_str]
while not (list[0]==list[1]==list[2]) :
    ma=max(list)
    mi=min(list)
    i_max=list.index(ma)
    i_min=list.index(mi)
    list[i_max]-=1
    list[i_min]+=1
    person+=1
    if person==100:
        print("NO SOLUTIONS")
        break
else:
    print(person)
